{
  "name": "webpack-5",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1"
  },
  "scripts": {
    "clean:install": "rimraf dist/ && rimraf node_modules/ && npm install",
    "build:dev": "webpack --config config/webpack.development.js",
    "build:prod": "webpack --config config/webpack.production.js",
    "build:analyze": "webpack --config config/webpack.analyze.js",
    "serve:dev": "webpack-dev-server --open --config config/webpack.development.js",
    "serve:prod": "serve dist/",
    "analyze": "npm run clean:install && npm run build:analyze",
    "dev": "npm run clean:install && npm run build:dev && npm run serve:dev",
    "start": "npm run clean:install && npm run build:prod && npm run serve:prod"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "rimraf": "3.0.2",
    "serve": "^11.3.2",
    "style-loader": "^1.2.1",
    "webpack": "^5.0.0-beta.24",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.1"
  }
}
